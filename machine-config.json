{
"config":{
  "region": "sjc",
  "guest": {
    "cpu_kind": "shared",
    "cpus": 1,
    "memory_mb": 1024
  },
  "containers": [
    {
      "name": "shell",
      "image": "registry.fly.io/proxypilot:shell",
      "env": {
        "GH_TOKEN": "DUMMY"
      }
    },
    {
      "name": "sideproxy",
      "image": "registry.fly.io/proxypilot:sideproxy",
      "env": {
        "PROXY": "https://tokenizer.fly.io",
        "PROXYAUTH": "proxyauthtoken",
        "URL": "https://api.github.com",
        "PORT": "443"
      },
      "secrets": [
         { "name": "URLAUTH", "env_var": "URLAUTH" }
      ]
    }
  ]
}
}
