{
"config":{
  "region": "sjc",
  "guest": {
    "cpu_kind": "shared",
    "cpus": 1,
    "memory_mb": 1024
  },
  "image": "bogus",
  "containers": [
    {
      "name": "shell",
      "image": "registry.fly.io/proxypilot:shell",
      "env": {
        "GH_TOKEN": "TOKEN"
      }
    },
    {
      "name": "sideproxy",
      "image": "registry.fly.io/proxypilot:sideproxy",
      "env": {
        "PORT": "443"
      },
      "secrets": [
         { "name": "TOKEN", "env_var": "TOKEN" }
      ]
    }
  ]
}
}
