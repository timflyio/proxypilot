{
  "config": {
  "containers": [
    {
      "name": "sideproxy",
      "image": "registry.fly.io/proxypilot:sideproxy",
      "env": {
        "PORT": 443,
        "TOKEN": "TOKENTOKENTOKEN"
      },
      "health_checks": [
        {
          "type": "tcp",
          "port": 443
        }
      ]
    },
    {
      "name": "shell",
      "image": "registry.fly.io/proxypilot:shell",
      "depends_on": [
        {
          "container": "sideproxy",
          "condition": "healthy"
        }
      ]
    }
  ]
}
}
